<!DOCTYPE html>
<html lang="zh">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 吃货集中营</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    @Styles.Render("~/Scripts/Plugins/bootstrap/css/css")
    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="~/Scripts/Plugins/Html5/html5shiv.min.js"></script>
    <script src="~/Scripts/Plugins/Html5/respond.min.js"></script>
    <![endif]-->
    @RenderSection("HeadPartialSection", false)
</head>
<body>
    <header>
        <div class="navbar navbar-fixed-top navbar-inverse">
            <div class="container">
                <div class="navbar-collapse">
                    <ul class="nav nav-tabs">
                        <li><a href="#">fdsafsdafa</a></li>
                        <li class="navbar navbar-nav">
                            <a href="#">框架</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </header>
    <div id="body" class="container">
        @RenderSection("featured", false)
        <section class="content-wrapper main-content clear-fix">
            @RenderBody()
        </section>
    </div>
    <footer>
        <div class="navbar navbar-fixed-bottom navbar-inverse">
            底部
        </div>
    </footer>
    @Scripts.Render("~/bundles/jquery","~/bundles/bootstrap")
    @RenderSection("FootPartialSection", false)
</body>
</html>